#include <iostream>
using namespace std;


// QUESTION 7:
// Write a C++ program that reads in a string from the user and checks if it contains
// the character 'a'. If it does, print "Contains 'a'" to the screen, otherwise print
// "Does not contain 'a'".


int main() {
    string s;

    cout << "Enter a string: ";
    cin >> s;

    if (s.find('a') != string::npos) {
        cout << "Contains 'a'" << endl;
    } else {
        cout << "Does not contain 'a'" << endl;
    }

    return 0;
}
